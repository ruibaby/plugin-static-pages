{
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild build --env-mode development --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../.gitignore",
    "prettier": "prettier --write src/",
    "type-check": "vue-tsc --build --force",
    "test:unit": "echo 'No unit tests'"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "@formkit/vue": "^1.7.2",
    "@halo-dev/api-client": "^2.22.0",
    "@halo-dev/components": "^2.22.0",
    "@halo-dev/ui-shared": "^2.22.1",
    "@he-tree/tree-utils": "0.1.0-alpha.4",
    "@he-tree/vue": "^2.10.1",
    "@imengyu/vue3-context-menu": "^1.5.4",
    "@tanstack/vue-query": "4.37.1",
    "@vueuse/core": "^10.11.1",
    "@vueuse/router": "^10.11.1",
    "axios": "^1.13.5",
    "grapesjs": "^0.21.13",
    "grapesjs-blocks-basic": "^1.0.2",
    "grapesjs-plugin-forms": "^2.0.6",
    "grapesjs-preset-webpage": "^1.0.3",
    "grapesjs-tabs": "^1.0.6",
    "monaco-editor": "^0.54.0",
    "pretty-bytes": "^6.1.1",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@halo-dev/ui-plugin-bundler-kit": "^2.22.0",
    "@iconify/json": "^2.2.438",
    "@rsbuild/core": "1.6.6",
    "@rsbuild/plugin-vue": "^1.2.6",
    "@rushstack/eslint-patch": "^1.15.0",
    "@shikijs/markdown-it": "^1.29.2",
    "@tsconfig/node20": "^20.1.9",
    "@types/node": "^20.19.33",
    "@unocss/eslint-config": "66.5.6",
    "@unocss/webpack": "66.5.6",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "monaco-editor-webpack-plugin": "^7.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "shiki": "^1.29.2",
    "typescript": "~5.8.3",
    "unocss": "66.5.6",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-markdown": "^0.26.3",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^3.2.4"
  },
  "packageManager": "pnpm@10.29.2"
}
